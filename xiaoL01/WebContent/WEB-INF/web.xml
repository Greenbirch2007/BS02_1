<?xml version="1.0" encoding="UTF-8" ?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<welcome-file-list>
		<welcome-file>shain-manage</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/error?statusCode=404</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/error?statusCode=500</location>
	</error-page>
	<jsp-config>
      <taglib>
          <taglib-uri>/WEB-INF/customFunctions</taglib-uri>
          <taglib-location>/WEB-INF/custom_functions.tld</taglib-location>
      </taglib>
    </jsp-config>
</web-app>